/*---------- MENU BOTTOM ---------- */
#container_menu_bottom {margin:0px; float:none; overflow:hidden; background:none;}
#menu_bottom {width:980px; margin:20px auto 10px auto; padding:7px 10px 7px 10px; float:none; overflow:hidden; color:#cccccc; font-size:12px; font-weight:normal; text-transform:normal; background:none; border-top:1px solid #eeeeee;}
#menu_bottom a {color:#cccccc; text-decoration:none;}
#menu_bottom a:hover {color:#cccccc; text-decoration:underline;}

#footer {width:980px; margin:10px auto 10px auto; padding:8px 10px; background:#000000; color:#ffffff; font-size:11px; font-family:Arial; text-transform:normal; letter-spacing:1px; text-align:left; border-top:1px solid #dddddd; border-bottom:1px solid #dddddd; float:none; overflow:hidden;}
#footer a {color:#ffffff;}
#footer .navigation {float:left; overflow:hidden;}
#footer .projects {float:right; overflow:hidden;}

#donor {width:980px; margin:20px auto 10px auto; background:transparent; color:#777777; font-size:11px; font-family:Arial; text-transform:normal; text-align:center;}
