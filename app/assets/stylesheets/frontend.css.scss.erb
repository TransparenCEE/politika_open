/* GENERAL */
body {margin:0px; padding:0px; font-family: 'Open Sans', sans-serif; font-size:12px; color:#666666;}
h1 {margin:0px 0px 20px 0px; padding:0px; font-size:22px; font-weight:bold; color:#333333;}
a {color:#009900; text-decoration:none;}
a:hover {color:#009900; text-decoration:underline;}
img {border:none;}
a .info_tab {color:#666666; text-decoration:none;}
a .info_tab:hover {color:#666666; text-decoration:underline;}


/* HEADER */  
#container_header {margin:0px auto; padding:0px; background: transparent; float:none; overflow:hidden; width: 1000px;}
#header {width:1000px; margin:0px auto; padding:0px; float:none; overflow:hidden; color:#333333; font-family: 'Open Sans', sans-serif; font-size:12px;}
#header .logo {display:inline; margin:20px 0px 15px 0px; padding:5px 0px 0px 0px; height:36px; float:left; overflow:hidden;}
#header .navigation {display:inline; margin:50px auto 0px auto; padding:0px; float:right; overflow:hidden; color:#333333; font-family: 'Open Sans', sans-serif; font-weight:bold; font-size:12px; text-align:right;}
#header a {color:#333333; text-decoration:none;}
#header a:hover {text-decoration:underline;}


/* MENU */
#container_menu {font-family: 'Open Sans', sans-serif; background:transparent; float:none; overflow:hidden; margin:0px auto; padding:0px; width: 1000px;}
#menu {width:1000px; margin:0px auto 10px auto; font-size:12px; text-transform:normal; border:0px solid #065e06; background:#39B54A;}
#menu ul {list-style:none; float:none; overflow:hidden; margin:0px; padding:0px;}
#menu li {float:left; overflow:hidden; border-right:1px solid #ffffff;}
#menu a {display:block;	text-decoration:none; margin:0px; padding:12px 17px; color:#ffffff; font-weight:bold; background: transparent; text-transform:uppercase; letter-spacing:1px;}
#menu a.home {font-size:14px; font-weight:bold; color:#e9f9e9; letter-spacing:1px; background: -webkit-gradient(linear, left top, left bottom, from(#10b810), to(#009900)); background: -moz-linear-gradient(top, #10b810, #009900); padding:11px 10px; border-top-left-radius:0px; border-bottom-left-radius:0px;}
#menu a.user {text-decoration:none; margin:0px; padding:12px 10px; color:#ffffff; background:url(<%= asset_path 'frontend/bckmenu_users.png' %>) #009900;}
#menu a.admin {text-decoration:none;  margin:0px; padding:12px 10px; color:#ffffff; background:#B61D22; text-transform:uppercase;}
#menu a:hover {background:#000000;}
#menu a.admin:hover {color:#ffffff; background:#D3666E;}


/*---------- SEARCH  ----------*/
#container_search {float:none; overflow:hidden; width:350px; background:#ffffff; border:0px solid #cccccc; margin:4px 0px 40px 43px; padding:2px;}
#container_search .text_area {float:left; padding:0px; margin:7px 0px 0px 10px; height:30px; width:290px; font-size:18px; border:1px solid #ffffff; border:none; color:#333333;}
#container_search .submit_button {height:40px; width:40px; border:none; background: #009900 url(<%= asset_path 'button_search.png' %>); background-repeat:no-repeat; padding:0px; margin:0px; cursor:pointer; float:right; color:transparent;}
#container_search form {padding:0px; margin:0px;}

#home_banners a {color:#ffffff;}

/*---------- NEW SEARCH  ----------*/
#container_search_new {float:none; overflow:hidden; width:690px; background:#ffffff; border:1px solid #666666; margin:4px auto 40px auto; padding:0px;}
#container_search_new .text_area {float:left; padding:0px; margin:7px 0px 0px 10px; height:30px; width:300px; font-size:18px; border:1px solid #ffffff; border:none; color:#333333;}
#container_search_new .submit_button {border:none; background:#39B54A; padding:11px 20px; margin:0px; cursor:pointer; float:right; color:#ffffff; font-size:18px;}
#container_search_new .submit_button:hover {background:#000000;}
#container_search_new form {padding:0px; margin:0px;}


