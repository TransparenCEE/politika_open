/*---------- CONTAINER MAIN ---------- */
#container_main {width:1000px; padding:0px; float:none; overflow:hidden; background:none; margin: 0 auto 30px; }

/*---------- SUPPORT ---------- */ 
#container_support {background:#009900; color:#ffffff; margin:0px 20px 0px 0px; padding:30px; font-size:24px; width:350px; float:left; overflow:hidden;}
#container_support .email {padding:0px 0px 20px 0px; border-bottom:1px solid #ffffff;}
#container_support .phone {padding:20px 0px 0px 0px;}
#container_support a {color:#ffffff; text-decoration:none;}
#container_support a:hover {color:#ffffff; text-decoration:underline;}

/*---------- SUPPORT ---------- */
#rules li {margin:0px 0px 10px 0px;}



#zastupenie_strany ul {list-style:none; margin:0px 0px 0px 0px; padding:0px; float:none; display:block; overflow:hidden;}
#zastupenie_strany li {float:left; margin:0px 3px 0px 0px; padding:5px 0px 0px 0px; overflow:hidden;}




/*---------- LIST ---------- */
#abc {float:none; overflow:hidden; margin:0px 0px 10px 0px;}
#abc ul {list-style:none; margin:0px 0px 0px 0px; padding:0px;}
#abc li {float:left; margin:0px 3px 0px 0px; padding:7px 0px; width:28px; background:#999; text-align:center; color:#eee;}
#abc li:hover {float:left; margin:0px 3px 0px 0px; padding:7px 0px; width:28px; background:#666	; text-align:center; color:#fff;}

#list {float:none; overflow:hidden;}
#list table {border-collapse:collapse; width:100%; margin:0px 0px 0px 0px; color:#666; font-weight:normal;}
#list tr {background:#fff; font-size:12px; border-bottom:1px solid #ccc;}
#list tr.title {background:#009900; color:#fff; border-bottom:2px solid #fff; text-align:left; text-transform:uppercase; font-size:11px; font-family:Verdana, Arial, Helvetica, sans-serif;}
#list tr.title:hover {background:#000; color:#fff; }
#list tr.header {background:#009900; color:#fff; border-bottom:none; font-weight:bold;}
#list tr.header a { color: #fffeff; }
#list tr.header:hover {background:#009900; color:#fff; font-weight:bold;}
/* #list tr.odd {background:#efefef;} */
#list tr:hover {background:#cfcfcf; color:#fff}
#list td {padding:7px 10px; font-size:11px;}
#list td.right {text-align:right;}
#list td.priezvisko {font-weight:bold; letter-spacing:1px;}

/* @group Sorting */

td.sort {  }

td.sort.asc { background: url(<%= asset_path 'sort_icons.png' %>) no-repeat left -38px; }

td.sort.desc { background: url(<%= asset_path 'sort_icons.png' %>) no-repeat left 12px; }



/* @end */


